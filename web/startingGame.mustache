	 socket.emit('waitingSuscriber',{{channel}});
	 socket.on('suscriber',function(msg){
	 	var elem=msg.split(':');
	 	var channel=parseInt(elem[0]);
	 	var chn={{channel}};
	 	var message=elem[1];
	 	if ((channel==chn)&&(message=='OKEY')) document.forms['toPlay'].submit();
	 });
	
